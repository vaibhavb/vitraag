{
  "name": "myhomepage",
  "version": "1.0.0",
  "description": "My home page",
  "main": "assets/sass/mybulma.scss",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css-build": "sass --load-path=node_modules --silence-deprecation=import assets/sass/mybulma.scss assets/css/mybulma.css",
    "css-watch": "npm run css-build -- --watch",
    "serve": "bundle exec jekyll serve --livereload",
    "start": "npm-run-all --parallel css-watch serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhavb/vitraag.git"
  },
  "keywords": [
    "sass",
    "homepage",
    "jekyll"
  ],
  "author": "Vaibhav Bhandari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaibhavb/vitraag/issues"
  },
  "homepage": "https://github.com/vaibhavb/vitraag#readme",
  "devDependencies": {
    "bulma": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.53.0"
  }
}
