{
  "name": "mybulma",
  "version": "0.0.1",
  "description": "My Implementation of bulma",
  "main": "assets/sass/mybulma.scss",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "js-build": "webpack --mode development",
    "js-watch": "npm run js-build -- --watch",
    "css-build": "node-sass assets/sass/mybulma.scss assets/css/mybulma.css",
    "css-watch": "npm run css-build -- --watch",
    "serve": "bundle exec jekyll serve --livereload",
    "start": "npm-run-all --parallel css-watch js-watch serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhavb/vitraag.git"
  },
